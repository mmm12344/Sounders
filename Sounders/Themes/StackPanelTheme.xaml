    <Style TargetType="StackPanel" x:Key="roundedStack">
        <Style.Resources>
            <Style TargetType="Border">
                <Setter Property="CornerRadius" Value="20"/>
            </Style>
        </Style.Resources>
    </Style>
</ResourceDictionary>